{
    "name": "ambengers/eloquent-word",
    "description": "Laravel package for creating Word documents with Eloquent ORM dependencies.",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Marvin Quezon",
            "email": "quezon.marvin@gmail.com"
        }
    ],
    "homepage": "https://github.com/ambengers/eloquent-word",
    "keywords": ["Laravel", "Eloquent", "Word"],
    "require": {
        "illuminate/support": "~7|~8",
        "phpoffice/phpword": "^0.17.0",
        "spatie/laravel-medialibrary": "^7.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "provide": {
        "ext-imagick": "*"
    },
    "autoload": {
        "psr-4": {
            "Ambengers\\EloquentWord\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ambengers\\EloquentWord\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ambengers\\EloquentWord\\EloquentWordServiceProvider"
            ],
            "aliases": {
                "EloquentPdf": "Ambengers\\EloquentWord\\Facades\\EloquentPdf"
            }
        }
    }
}
